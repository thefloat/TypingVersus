      <Text id="btt-text" strokeType="OUTSIDE" strokeWidth="0.0" text="Bt Typing">
         <TilePane.margin>
            <Insets />
         </TilePane.margin></Text>
      <Button id="blue-btn" fx:id="homeBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#homeBtnActn" text="NEXT" textFill="WHITE">
